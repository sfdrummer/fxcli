// _{{ filename }}.scss

{% for key, data in config %}
$ft--{{ key }}: {{ data.stack }};
{% for label, weight in data.weights %}
$wt--{{ key }}--{{ label }}: {{ weight }};
{% endfor %}
{% endfor %}

${{ filename }}: (
{% for key, data in config %}
    'ft--{{ key }}': $ft--{{ key }},
{% for label, weight in data.weights %}
    'wt--{{ key }}--{{ label }}': $ft--{{ key }}--{{ label }},
{% endfor %}
{% endfor %}
);
